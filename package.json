  "description": "Happeero",
  "scripts": {
    "ng": "ng",
    "build:universal": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:universal": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --aot --locale fr && ng build --aot npm run --app 1 --output-hashing=false",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "server": "nodemon bin/www",
    "server:test": "nodemon --exec 'mocha -R min'",
    "start:aot": "ng serve --aot --i18n-file src/locale/messages.en.xlf --locale en --i18n-format xlf",
    "build:aot": "ng build --aot",
    "socket": "nodemon bin/socket",
    "prod": "ng build --prod --locale fr",
    "prod:en": "ng build --prod --output-path dist/en --bh /en/ --i18n-file src/locale/messages.en.xlf --locale en --i18n-format xlf",
    "dev": "ng serve --locale fr",
    "dev:aot": "ng serve --aot --locale fr",
    "compile_@agm_core": "babel node_modules/@agm/core -d node_modules/@agm/core --presets es2015",
    "compile_ngslick": "babel node_modules/ngx-slick -d node_modules/ngx-slick --presets es2015",
    "compile_toaster": "babel node_modules/angular2-toaster -d node_modules/angular2-toaster --presets es2015",
    "compile_uploader": "babel node_modules/ngx-uploader -d node_modules/ngx-uploader --presets es2015",
    "postinstall": "npm run compile_@agm_core && npm run compile_ngslick",
    "i18n": "ng-xi18n -p ./tsconfig.json --i18nFormat=xlf"
  },
  "keywords": [
    "happeero"
  ],
